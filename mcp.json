{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "OpenZeppelinSolidityContracts": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://mcp.openzeppelin.com/contracts/solidity/mcp"
      ]
    },
    "GitHub": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-github"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN:-}"
      }
    },
    "Playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-playwright"
      ]
    },
    "Apidog": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://mcp.apidog.com/server"
      ]
    },
    "Supabase": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-supabase"
      ],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL:-}",
        "SUPABASE_ANON_KEY": "${SUPABASE_ANON_KEY:-}"
      }
    }
  }
}
